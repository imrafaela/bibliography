@article{ WOS:001426820300001,
Author = {Shi, Jing and Zhang, Nan and Wang, Wenhao and Zhang, Menglin and Cai,
   Gangwei and Gao, Weijun},
Title = {The effect of the preferred illuminance on dynamic features of the brain
   activity during reading},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2025},
Volume = {103},
Month = {JUN 1},
Abstract = {The light environment constitutes an important facet of indoor
   environmental quality, intimately intertwined with the well-being and
   productivity of occupants. Electroencephalogram (EEG) offers distinct
   advantages in assessing preferred illuminance over subjective
   evaluations. A correlation model was constructed using EEG dynamic
   features to clarify the connection between preferred illuminance and
   brain activity during reading. EEG data were collected from 28 college
   students exposed to three typical illuminances (200 lx, 500 lx, and 1000
   lx). The participants were stratified based on their preferred
   illuminance, as indicated by the frontal asymmetry index. Notably,
   individual variances in preferred illuminance were observed. Compared
   with nonpreferred illuminance, brain power significantly decreased by
   3.72-32.52 \% under preferred illuminance, particularly in the parietal
   and occipital lobes. The effective delay time of the autocorrelation
   alpha oscillations increased by 0.13-9.11 \%, while the critical index
   of neural avalanches decreased by 13.89-42.04 \%. These indicate the
   more comfortable and healthier brain state. Five dynamic features
   derived from brain oscillations and avalanche activity effectively
   predicted preferred illuminance, with the K-nearest neighbors algorithm
   achieving a superior classification accuracy of 77.78 \%. This study
   proposes an innovative approach to assess preferred illuminance and
   provides theoretical support for a comprehensive understanding of the
   impact of illuminance on brain activity, facilitating the formulation of
   individualized lighting comfort models, and promoting resource
   efficiency and smart indoor environment management.},
