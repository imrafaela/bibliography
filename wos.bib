@article{ WOS:000899491000002,
Author = {Pan, Liling and Zheng, Hanying and Li, Tingxun},
Title = {Effects of the indoor environment on EEG and thermal comfort assessment
   in males},
Journal = {BUILDING AND ENVIRONMENT},
Year = {2023},
Volume = {228},
Month = {JAN 15},
Abstract = {This study investigated the effect of indoor thermal conditions on
   electroencephalogram (EEG) signals and used logistic regression to
   discriminate indoor, thermal comfort. Twenty male subjects were
   recruited to record resting EEG signals under thermal conditions
   consisting of a combination of temperature, relative humidity, and air
   velocity. Subjective questionnaires were used to collect individuals'
   perceptions of the environment and as classification criteria for
   developing logistic regression models. The results indicated that at 22
   degrees C and 25 degrees C, theta, beta 1, beta 2, and gamma waves were
   highly similar, and velocity was also highly similar for all EEG waves
   at 0.5 and 1 m/s. For beta 1, beta 2, and gamma, 70\% RH can be used as
   the high humidity cut-off point in the humidity test. In addition,
   regression models for indoor comfort discrimination utilizing the
   frequency bands relevant to comfort are regressed, and the total model
   revealed that 88.6\% of the data were correctly categorized. This
   research serves as a starting point for further research into the
   coupled environment and neuronal mechanisms.},
